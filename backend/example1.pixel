# rect and circle matrix #
a = rect(50, 50, 20, 20, red)
red = color(255, 0, 0)

dd = 30
while (a.x < 400) {
    a.y = 50
    red.R = a.x
    red.G = a.x / 255
    while (a.y < 300) {
        draw a
        a.y = a.y + dd
    }
    a.x = a.x + dd
}

b = circle(65, 75, 4, blue)
blue = color(200,200,200)
b.r = 5
while (b.x < 395) {
    b.y = 66
    blue.B = b.x / 255
    blue.G = b.x
    while (b.y < 295) {
        draw b
        b.y = b.y + dd
        b.color = blue
    }
    b.x = b.x + dd
}

