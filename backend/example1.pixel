haha = func(int xx, int yy, int rr) {
    blue = color(0, 0, 255)
    c.x = xx
    c.y = yy
    c.r = rr
    
    if (xx == 200){
        c.color = red
    }
    else{
        c.color = blue
    }
    draw c
    
    foo = func(int aa, int bb) {
        cir = rect(300,100,50, 50, blue)
        cir.x = aa
        cir.y = bb
        draw cir
    }

    ax = 150
    bx = 200
    call foo(ax, bx)

}

