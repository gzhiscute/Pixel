#a wheel#

a = circle(0, 0, 10, red)
red = color(255, 0, 100)

x = 150
y = 150
r = 50

while (a.x < 400) {
    
    a.y = 0
    while (a.y < 400) {
        if ((a.x-x)*(a.x-x)+(a.y-y)*(a.y-y) == r*r) {
            draw a
        }
        else {}
        a.y = a.y + 1
    }
    a.x = a.x + 1
}

